{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "eslint-config-prettier"
  ],
  "rules": {
    "no-unused-vars": 1,
    "no-console": 0,
    "react/prop-types": 0,
    "react/display-name": 0,
    "jsx-a11y/anchor-is-valid": [
      0,
      {
        "components": ["Link"],
        "specialLink": ["to"]
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
